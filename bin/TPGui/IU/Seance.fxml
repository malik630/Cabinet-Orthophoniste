<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="TPGui.IU.SeanceController">
	<children>
		<TextField fx:id="heureField" layoutX="276.0" layoutY="105.0"
			promptText="Heure" />
		<DatePicker fx:id="dateField" layoutX="265.0" layoutY="68.0"
			promptText="Entrez une date" />
		<Button layoutX="272.0" layoutY="145.0" mnemonicParsing="false"
			onAction="#rechercherRDV" text="Rechercher le rendez-vous">
			<font>
				<Font name="Tahoma" size="12.0" />
			</font>
		</Button>
		<AnchorPane prefHeight="60.0" prefWidth="700.0"
			style="-fx-background-color: #FFF8DC;">
			<children>
				<Label layoutX="223.0" layoutY="15.0" text="Commencer une séance">
					<font>
						<Font name="Tahoma" size="24.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
		<AnchorPane fx:id="pane1" layoutX="482.0" layoutY="68.0"
			prefHeight="77.0" prefWidth="200.0" style="-fx-background-color: #84DBF0;"
			visible="false">
			<children>
				<TextField fx:id="adresseField" layoutX="26.0" layoutY="60.0"
					promptText="Adresse" />
				<TextField fx:id="dateDeNaissanceField" layoutX="26.0"
					promptText="Date de naissance" />
				<TextField fx:id="lieuNaissanceField" layoutX="26.0"
					layoutY="30.0" promptText="Lieu de naissance" />
			</children>
		</AnchorPane>
		<AnchorPane fx:id="pane2" layoutX="482.0" layoutY="158.0"
			prefHeight="85.0" prefWidth="200.0" style="-fx-background-color: #84DBF0;"
			visible="false">
			<children>
				<TextField fx:id="numPersonnelField" layoutX="26.0"
					layoutY="60.0" promptText="Numéro personnel" />
				<TextField fx:id="professionField" layoutX="26.0" layoutY="30.0"
					promptText="Profession" />
				<TextField fx:id="diplomeField" layoutX="26.0" promptText="Diplôme" />
			</children>
		</AnchorPane>
		<AnchorPane fx:id="pane3" layoutX="482.0" layoutY="251.0"
			prefHeight="60.0" prefWidth="200.0" style="-fx-background-color: #84DBF0;"
			visible="false">
			<children>
				<TextField fx:id="classeEtudeField" layoutX="26.0"
					promptText="Classe d'étude" />
				<TextField fx:id="numParentField" layoutX="26.0" layoutY="30.0"
					promptText="Numéro téléphone tuteur" />
			</children>
		</AnchorPane>
		<AnchorPane fx:id="pane5" layoutX="252.0" layoutY="186.0"
			prefHeight="120.0" prefWidth="200.0" style="-fx-background-color: #84DBF0;"
			visible="false">
			<children>
				<Button layoutX="42.0" layoutY="143.0" mnemonicParsing="false"
					onAction="#terminerSeance" text="Terminer la séance">
					<font>
						<Font name="Tahoma" size="12.0" />
					</font>
				</Button>
				<TextArea fx:id="observationField" prefHeight="131.0"
					prefWidth="200.0" promptText="Régigez l'observation du rendez-vous" />
			</children>
		</AnchorPane>
		<Button layoutX="324.0" layoutY="363.0" mnemonicParsing="false"
			onAction="#switchToScene3" text="Retour">
			<font>
				<Font name="Tahoma" size="12.0" />
			</font>
		</Button>
		<Button fx:id="ficheSuiviButton" layoutX="29.0" layoutY="93.0"
			mnemonicParsing="false" onAction="#switchToScene7" text="Rédiger une fiche de suivi"
			visible="false">
			<font>
				<Font name="Tahoma" size="12.0" />
			</font>
		</Button>
		<AnchorPane fx:id="pane4" layoutX="472.0" layoutY="327.0"
			prefHeight="72.0" prefWidth="219.0" style="-fx-background-color: #84DBF0;"
			visible="false">
			<children>
				<Button layoutX="7.0" layoutY="14.0" mnemonicParsing="false"
					onAction="#switchToScene9" text="Commencer un bilan orthophonique">
					<font>
						<Font name="Tahoma" size="12.0" />
					</font>
				</Button>
				<Button layoutX="80.0" layoutY="49.0" mnemonicParsing="false"
					onAction="#annuler" text="Annuler">
					<font>
						<Font name="Tahoma" size="12.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
		<Button fx:id="ajouterPatient" layoutX="526.0" layoutY="311.0"
			mnemonicParsing="false" onAction="#ajouterPatient" text="Ajouter un patient"
			visible="false">
			<font>
				<Font name="Tahoma" size="12.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
